    <div class="dropdown ac">
      <button
        class="btn btn-dark dropdown-toggle"
        type="button"
        id="dropdownMenuButton1"
        data-bs-toggle="dropdown"
        aria-expanded="false"
      >
        Create/Join Class <i class="fas fa-users-cog"></i>
      </button>
      <ul class="dropdown-menu bg-dark" aria-labelledby="dropdownMenuButton1">
        <li>
          <a
            class="dropdown-item drp"
            type="button"
            class="edit btn btn-success pull-left"
            data-bs-toggle="modal"
            data-bs-target="#CreateC"
            >Create Class</a
          >
        </li>
        <li>
          <a
            class="dropdown-item drp"
            type="button"
            class="edit btn btn-success pull-left"
            data-bs-toggle="modal"
            data-bs-target="#Join"
            >Join Class</a
          >
        </li>
      </ul>
    </div>
    <!-- create class -->
    <div
      class="modal fade"
      id="CreateC"
      tabindex="-1"
      aria-labelledby="exampleModalLabel"
      aria-hidden="true"
    >
      <div class="modal-dialog">
        <div class="modal-content bg-dark">
          <div class="modal-header">
            <h5
              class="modal-title"
              id="exampleModalLabel"
              style="color: aliceblue"
            >
              Create Class
            </h5>
            <button
              type="button"
              class="btn-close"
              data-bs-dismiss="modal"
              aria-label="Close"
            ></button>
          </div>
          <div class="modal-body">
            <label for="CreateClass" style="color: aliceblue"
              ><strong>Class Name :</strong></label
            >
            <input
              type="text"
              name="CreateClass"
              id="CreateClass"
              placeholder="Enter your class name"
              style="background-color: #181818; color: white"
            />
            <br />
            <label for="Description" style="color: aliceblue"
              ><strong>Description :</strong></label
            >
            <input
              type="text"
              name="Description"
              id="Description"
              placeholder="Let people know what this class about"
              style="background-color: #181818; color: white"
            />
            <br />
            <label for="Privacy" style="color: aliceblue"
              ><strong>Privacy :</strong></label
            >
            <select
              id="Privacy"
              name="Privacy"
              style="background-color: #181818; color: white"
            >
              <option value="Private">
                Private - Only class owner can add members
              </option>
              <option value="Public">
                Public - Anyone can join your class
              </option>
            </select>
          </div>
          <div class="modal-footer">
            <button
              type="button"
              class="btn btn-secondary"
              data-bs-dismiss="modal"
            >
              Close
            </button>
            <button
              type="button"
              class="btn btn-primary"
              data-bs-toggle="modal"
              data-bs-target="#SuccessCreateClass"
            >
              Create
            </button>
          </div>
        </div>
      </div>
    </div>
    <!-- join class -->
    <div
      class="modal fade"
      id="Join"
      tabindex="-1"
      aria-labelledby="exampleModalLabel"
      aria-hidden="true"
    >
