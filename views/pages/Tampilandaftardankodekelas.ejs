        <div class="modal-content bg-dark">
          <div class="modal-body">
            <h3 style="text-align: center; color: aliceblue">
              Enter Your Class Code
            </h3>
            <div class="d-flex justify-content-center">
              <lottie-player
                src="https://assets3.lottiefiles.com/packages/lf20_jvj9d2lc.json"
                background="transparent"
                speed="1"
                style="width: 300px; height: 300px"
                loop
                autoplay
              ></lottie-player>
            </div>
            <div class="d-flex justify-content-center">
              <input
                type="text"
                placeholder="Enter Code"
                style="background-color: #181818; color: white; width: 200px"
              />
            </div>
          </div>
          <div>
            <button
              type="button"
              class="btn btn-primary pull-right mx-2 my-2"
              data-bs-toggle="modal"
              data-bs-target="#SuccessAddClass"
            >
              Enter
            </button>
            <button
              type="button"
              class="btn btn-secondary pull-right my-2"
              data-bs-dismiss="modal"
            >
              Close
            </button>
          </div>
        </div>
      </div>
    </div>

